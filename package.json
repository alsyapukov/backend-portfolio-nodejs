{
  "name": "fullstack_vue_express",
  "version": "1.0.0",
  "description": "Full stack vue and express app",
  "main": "index.js",
  "scripts": {
    "start": "node server/index.js",
    "serve": "nodemon index.js --exec babel-node --presets @babel/preset-env",
    "babel-node": "babel-node",
    "build": "NODE_ENV=production babel src --out-dir dist --source-maps inline",
    "prepublishOnly": "npm run build"
  },
  "author": "Brad Traversy",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^4.0.1",
    "body-parser": "^1.20.1",
    "cors": "^2.8.4",
    "express": "^4.18.2",
    "gm": "^1.23.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.1.6",
    "mongoose": "^5.9.5",
    "multer": "^1.4.2",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "nodemon": "^1.19.4"
  }
}
